{
    "name": "inventor96/mako-vue",
    "version": "1.3.0",
    "description": "Opinionated boilerplate for web apps using PHP/Mako on the backend, and Vue on the frontend.",
    "keywords": [
        "mako",
        "framework",
        "vue",
        "boilerplate",
        "inertia"
    ],
    "homepage": "https://github.com/inventor96/mako-vue",
    "license": "MIT",
    "authors": [
        {
            "name": "Caleb Hornbeck",
            "homepage": "https://calebhornbeck.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "app\\": "app"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "mako/framework": "10.0.*",
        "inventor96/inertia-mako": "^1.4",
        "inventor96/mako-mailer": "^1.0",
        "inventor96/mako-template-pp": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-create-project-cmd": [
            "php app/reactor app:generate-secret",
            "php app/reactor openssl:generate-key",
            "php app/reactor gatekeeper:generate-auth-key",
            "php app/reactor session:generate-name",
            "php app/reactor git:update-ignore",
            "php app/reactor post-create-project"
        ]
    }
}
